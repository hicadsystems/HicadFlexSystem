@model IEnumerable<Flex.Data.ViewModel.UserAuthViewModel>

<ul class="nav nav-tabs border-none">
    <li role="presentation" class="active">
        <a class="border-none uppercase bold padding-horizontal-md margin-right-xs">Users</a>
    </li>
    <li class="pull-right">
        <button type="button" class="btn btn-success btn-md" onclick="loadUserInput(this)" data-url="@Url.Action("UserInput","UserRole")" ><span class="fa fa-plus" ></span> &nbsp; ADD USER</button>
    </li>
</ul>

<div class="margin-top-md">
    <span class="inline-block pull-right text-gray" style="margin-bottom:-34px; width:24px; height:34px; line-height:34px; vertical-align:middle;">
        <span class="fa fa-search"></span>
    </span>
   @* <input type="text" name="userSearch" style="padding-right:30px;" ng-model="searchText" ng-init="searchText=''" class=" form-control input-md"  placeholder="Search Users" />*@
    <input type="text" id="userSearch" style="padding-right:30px;" onkeyup="searchUser(document.getElementById('userSearch').value)" class=" form-control input-md"  placeholder="Search Users" />
</div>

<div class="row margin-top-md" id="tbUsers">
    @Html.Partial("_tbUsers", Model)
</div>
